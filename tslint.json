{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "max-line-length": [true, 250],
    "max-classes-per-file":false,
    "array-type": [true, "array"],
    "indent": [true, "spaces"],
    "ter-indent": [true, 2],
    "no-empty-interface": false,
    "quotemark": false,
    "no-trailing-whitespace": [true, "ignore-jsdoc"],
    "valid-jsdoc": [false, {
      "prefer": {
        "return": "return"
      },
      "requireReturn": false
    }],
    "no-angle-bracket-type-assertion": false,
    "member-ordering": [true, {
      "order": "fields-first"
    }],
    "forin": false
  },
  "rulesDirectory": []
}
